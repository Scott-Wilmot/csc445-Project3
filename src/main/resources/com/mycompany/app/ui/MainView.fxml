<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Accordion?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TitledPane?>

<Accordion fx:controller="com.mycompany.app.ui.uiController.MainController"
           xmlns:fx="http://javafx.com/fxml/1"
           xmlns="http://javafx.com/javafx/23.0.1"
           prefHeight="400.0" prefWidth="600.0">
  <panes>
    <TitledPane animated="false" text="Hello World">
         <content>
             <Button fx:id="myButton" mnemonicParsing="false" text="Click Me" onAction="#handleClick"/>
         </content></TitledPane>
    <TitledPane animated="false" text="Soney Testing this stuff" />
  </panes>
</Accordion>
